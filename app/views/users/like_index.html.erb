<div class="row">
	<div class="col-md-6 row">
		<div class="col-md-1"></div>
		<div class="like_index_box col-md-10">
			<h2 style="font-family: Impact;">Sounds</h2>
			<% @sounds.each do |sound| %>
				<div class="like_box">
				<%= link_to sound_path(sound.id) do %>
					<%= attachment_image_tag sound.user, :image, fallback: "/no_image.jpg", class: "like_image"%><br>
				<ul>
					<li>Title: <%= sound.title.truncate(13) %></li>
					<li>M <%= sound.tag1 %></li>
					<li>F <%= sound.tag2 %></li>
					<li>P <%= sound.tag3 %></li>
				</ul>
				<% end %>
				</div>
			<% end %>
		</div>
		<div class="col-md-1"></div>
	</div>

	<div class="col-md-6 row">
		<div class="col-md-1"></div>
		<div class="like_index_box col-md-10">
			<h2 style="font-family: Impact;">Users</h2>
			<% @users.each do |user| %>
				<div class="like_box">
				<%= link_to user_path(user.id) do %>
					<%= attachment_image_tag user, :image, fallback: "/no_image.jpg", class: "like_image"%><br>
					<ul>
						<li><%= user.name %></li>
					</ul>
				<% end %>
				</div>
			<% end %>
		</div>
		<div class="col-md-1"></div>
	</div>
</div>